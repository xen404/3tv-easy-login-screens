{
  "name": "@groot/falbala",
  "version": "0.9.14",
  "description": "UI library for www.drei.at",
  "homepage": "https://portalsit.drei.at/de/falbala/",
  "scripts": {
    "cleanup": "node_modules/.bin/gulp cleanup",
    "build": "node_modules/.bin/gulp build",
    "start": "node_modules/.bin/gulp build && node server.js",
    "start:http": "node_modules/.bin/gulp build && node server.js http",
    "dev": "node_modules/.bin/gulp dev",
    "fix": "npm run fix:js && npm run fix:css",
    "fix:js": "./node_modules/standard/bin/cmd.js 'src/**/*.js' --fix",
    "fix:css": "./node_modules/stylelint/bin/stylelint.js '(src|components)/**/*.scss' '!src/documentation/**' --syntax scss --fix",
    "postinstall": "node ./postinstall.js"
  },
  "author": {
    "name": "Team Groot",
    "email": "AT.GROOT@drei.com",
    "url": "https://www.drei.at/"
  },
  "publishConfig": {
    "registry": "https://artifactory.three.com/artifactory/api/npm/npm/"
  },
  "main": "fractal.js",
  "license": "PROPRIETARY",
  "repository": {
    "type": "git",
    "url": "https://bitbucket.drei.com/projects/MP/repos/groot-falbala/"
  },
  "engines": {
    "node": ">=12",
    "npm": ">=6"
  },
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@babel/preset-env": "^7.13.8",
    "@frctl/fractal": "1.5.1",
    "@frctl/mandelbrot": "1.3.0",
    "autoprefixer": "^10.2.4",
    "del": "^6.0.0",
    "gulp": "^4.0.2",
    "gulp-better-rollup": "^4.0.1",
    "gulp-csso": "^4.0.1",
    "gulp-dart-sass": "^1.0.2",
    "gulp-debug": "^4.0.0",
    "gulp-if": "^3.0.0",
    "gulp-plumber": "^1.2.1",
    "gulp-rename": "^2.0.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-standard": "^14.0.0",
    "gulp-stylelint": "^13.0.0",
    "intersection-observer": "0.11.0",
    "marked": "^2.0.1",
    "micromodal": "^0.4.6",
    "onchange": "^7.1.0",
    "postcss-cli": "^8.3.1",
    "rollup": "^2.40.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "smoothscroll-polyfill": "0.4.4",
    "stylelint": "^13.11.0",
    "stylelint-config-standard-scss": "^1.1.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.19.0",
    "yall-js": "3.2.0",
    "yargs": "^16.2.0"
  },
  "dependencies": {
    "koa": "^2.13.1",
    "koa-static": "^5.0.0",
    "mkdirp": "^0.5.5"
  },
  "bundledDependencies": [
    "koa",
    "koa-static",
    "mkdirp"
  ],
  "browserslist": [
    "last 2 versions",
    "IE >= 11"
  ],
  "files": [
    "/src",
    "/build/core",
    "server.js",
    "health.js",
    "postinstall.js"
  ],
  "standard": {
    "ignore": [
      "src/documentation/**"
    ]
  }
}
